{
    "div": {
        "app": {
            "display": "flex",
            "background-color": "var(--main-bg-color)",
            "flex-direction": "column",
            "min-height": "100vh",
            "& *, input": {
                "font-family": "var(--main-font-family)",
                "font-size": "var(--main-font-size)",
                "color": "var(--main-color)"
            },
            "& h1, h3": {
                "font-weight": "bold"
            },
            "& h1": {
                "font-size": "var(--h1-font-size)"
            },
            "& h3": {
                "font-size": "var(--h3-font-size)"
            }
        },
        "user-profile": {
            "display": "flex",
            "align-items": "center",
            "padding": "20px"
        },
        "main-header": {
            "display": "flex",
            "align-items": "center",
            "flex-direction": "row",
            "justify-content": "space-between"
        },
        "user-information": {
            "display": "flex",
            "flex-direction": "row",
            "padding": "50px",
            "border-radius": "var(--main-border-radius)",
            "background": "var(--secondary-bg-color)"
        },
        "user-information-container": {
            "display": "flex",
            "flex-direction": "column",
            "flex": 1,
            "& > *:not(:last-child)": {
                "margin-bottom": "30px"
            }
        },
        "additional-information": {
            "display": "flex",
            "flex-direction": "column"
        },
        "additional-information-container": {
            "display": "flex",
            "flex-direction": "row",
            "justify-content": "space-between",
            "align-items": "flex-start",
            "padding": "20px",
            "border": "var(--main-border)",
            "border-color": "var(--main-border-color)",
            "background": "var(--secondary-bg-color)",
            "border-radius": "var(--main-border-radius)",
            "margin-top": "10px"
        },
        "user-full-name": {
            "display": "flex",
            "flex-direction": "row"
        },
        "posts-subscribers-buttons": {
            "display": "flex",
            "flex-direction": "row",
            "justify-content": "space-between",
            "width": "100%"
        },
        "post-creator-fields": {
            "background": "inherit",
            "& *": {
                "margin": "0 0 10px 0"
            }
        },
        "post-creator-visibility": {
            "background": "inherit",
            "& > *": {
                "margin": "0 0 5px 0"
            }
        },
        "post-creator-button": {
            "display": "flex",
            "justify-content": "flex-end"
        },
        "posts-post-title-settings": {
            "display": "flex",
            "justify-content": "space-between",
            "width": "100%"
        },
        "posts-post-follow": {
            "display": "flex",
            "flex-direction": "row",
            "& > *:first-child": {
                "margin-right": "10px"
            }
        },
        "registration-form-greeting": {
            "display": "flex",
            "flex-direction": "row",
            "align-items": "center",
            "& > *:first-child": {
                "margin-right": "10px"
            }
        },
        "registration-form": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "padding": "25px",
            "background": "var(--secondary-bg-color)",
            "border-radius": "var(--main-border-radius)",
            "width": "375px",
            "& > *:not(:last-child)": {
                "margin-bottom": "20px"
            }
        },
        "login-form": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "padding": "25px",
            "background": "var(--secondary-bg-color)",
            "border-radius": "var(--main-border-radius)",
            "width": "375px",
            "& > *:not(:last-child)": {
                "margin-bottom": "20px"
            }
        },
        "registration-form-wrapper": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "justify-content": "center"
        },
        "login-form-wrapper": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center",
            "justify-content": "center"
        },
        "post-container": {
            "display": "flex",
            "flex-direction": "column",
            "flex": "1",
            "& > *:not(:last-child)": {
                "margin-bottom": "20px"
            }
        },
        "label-with-icon-content": {
            "text-transform": "uppercase"
        },
        "account-photo-constructor": {
            "position": "relative",
            "height": "200px",
            "margin-right": "50px"
        },
        "account-photo-constructor-wrapper": {
            "position": "absolute",
            "top": "50%",
            "left": "50%",
            "transform": "translate(-50%, -50%)"
        },
        "registration-form-privacy": {
            "display": "flex",
            "width": "100%",
            "justify-content": "flex-start",
            "& > *:not(:last-child)": {
                "margin-right": "10px"
            }
        },
        "post-updater-fields": {
            "background": "inherit",
            "& > *": {
                "margin": "0 0 10px 0"
            }
        },
        "post-updater-button": {
            "display": "flex",
            "justify-content": "flex-end",
            "align-items": "center",
            "& > *:first-child": {
                "margin-right": "10px"
            }
        },
        "comment-container": {
            "display": "flex",
            "flex-direction": "column",
            "& > *:not(:last-child)": {
                "margin-bottom": "10px"
            }
        },
        "comment-section": {
            "display": "flex",
            "flex-direction": "column",
            "& > *:not(:last-child)": {
                "margin-bottom": "20px"
            }
        },
        "create-comment": {
            "display": "flex",
            "flex-direction": "row",
            "align-items": "flex-end",
            "& > *:not(last-child)": {
                "margin-right": "10px"
            }
        },
        "search-panel-fields": {
            "display": "flex",
            "flex-direction": "column",
            "& > *:not(:last-child)": {
                "margin-bottom": "10px"
            }
        },
        "not-found-container": {
            "display": "flex",
            "flex-direction": "column",
            "align-items": "center"
        },
        "toggle": {
            "& label": {
                "display": "flex",
                "align-items": "center",
                "flex-grow": "1",
                "padding": "5px",
                "background": "var(--main-bg-color)",
                "border-radius": "50px",
                "cursor": "pointer"
            },
            "& label > span": {
                "padding": "10px",
                "background": "var(--main-color)",
                "border-radius": "50%",
                "border": "0px"
            },
            "& input[type=checkbox]:checked ~ label": {
                "justify-content": "end"
            },
            "& input[type=checkbox]": {
                "display": "none"
            }
        },
        "toggle-wrapper": {
            "display": "flex",
            "align-items": "center",
            "& > *:first-child": {
                "width": "60px",
                "margin-right": "10px"
            }
        },
        "chats": {
            "display": "flex",
            "background": "var(--secondary-bg-color)",
            "border-radius": "var(--main-border-radius)",
            "padding": "5px"
        },
        "list-of-users": {
            "width": "375px"
        },
        "chat": {
            "width": "775px"
        },
        "chat-window": {
            "display": "flex",
            "flex-direction": "column",
            "height": "69vh",
            "background": "var(--main-bg-color)",
            "padding": "20px"
        },
        "message": {
            "background": "var(--secondary-bg-color)",
            "padding": "20px",
            "border-radius": "var(--main-border-radius)",
            "width": "70%",
            "& :first-child": {
                "display": "flex",
                "justify-content": "space-between",
                "align-items": "flex-end"
            },
            "&:not(:last-child)": {
                "margin-bottom": "20px"
            }
        },
        "chat-settings": {
            "& > *:first-child": {
                "padding": "20px"
            },
            "& > *": {
                "padding": "0 20px 20px"
            }
        },
        "change-photo": {
            "display": "flex",
            "align-items": "center"
        },
        "search-user": {
            "height": "69vh"
        }
    }
}